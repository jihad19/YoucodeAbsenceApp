        <Border  Grid.ColumnSpan="2" HorizontalAlignment="Left" Height="1045" Grid.RowSpan="6" VerticalAlignment="Top" Width="286" Background="White" CornerRadius="30,0,0,30"/>

        <!--Youcode logo-->
        <StackPanel x:Name="Logo" Grid.Column="1"  Orientation="Horizontal" Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="left" Height="65.258"  Width="168" Margin="0,29.871">
            <TextBlock HorizontalAlignment="Left" Height="68" TextWrapping="Wrap" VerticalAlignment="Top" Width="66" FontSize="35"  FontFamily="{StaticResource PoppinsRegular}" Text="You"/>
